<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.websquareproject.category.mapper.CategoryMapper">

    <select id="getCategories" resultType="com.example.websquareproject.category.dto.CategoryNameDto">
    SELECT category_name AS categoryName FROM CATEGORY WHERE parent_id IS NULL
    </select>
</mapper>
