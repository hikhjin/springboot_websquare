        			</w2:keyInfo>
        		</w2:dataMap>
        		<w2:dataList baseNode="map" id="dlt_excelList">
        			<w2:columnInfo>
        				<w2:column id="displayOrder" name="전시순서" dataType="text"></w2:column>
        				<w2:column id="type" name="구분" dataType="text"></w2:column>
        				<w2:column id="category1" name="카테고리1" dataType="text"></w2:column>
        				<w2:column id="category2" name="카테고리2" dataType="text"></w2:column>
        				<w2:column id="title" name="제목" dataType="text"></w2:column>
        				<w2:column id="views" name="조회수" dataType="text"></w2:column>
        				<w2:column id="isDisplayed" name="전시여부" dataType="text"></w2:column>
        				<w2:column id="displayStart" name="전시시작일시" dataType="text"></w2:column>
