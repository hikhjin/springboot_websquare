        				<w2:column id="pcImageUrl" name="pc이미지" dataType="text"></w2:column>
        				<w2:column id="pcImageAltText" name="pc이미지 대체텍스트" dataType="text"></w2:column>
        				<w2:column id="mobileImageUrl" name="모바일이미지" dataType="text"></w2:column>
        				<w2:column id="mobileImageAltText" name="모바일이미지 대체텍스트" dataType="text"></w2:column>
        				<w2:column id="sourceMediaList" name="출처매체" dataType="text"></w2:column>
        				<w2:column id="travelPlaceList" name="추천 여행지" dataType="text"></w2:column>
        				<w2:column id="attachmentUrl" name="첨부파일 경로" dataType="text"></w2:column>
        				<w2:column id="updatedAt" name="수정일시" dataType="text"></w2:column>
        				<w2:column id="updatedBy" name="수정자" dataType="text"></w2:column>
        				<w2:column id="createdAt" name="등록일시" dataType="text"></w2:column>
        				<w2:column id="createdBy" name="등록자" dataType="text"></w2:column>
        			</w2:columnInfo>
