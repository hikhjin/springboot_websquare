        <w2:layoutInfo/>
        <w2:publicInfo method=""/>
        <script lazy="false" type="text/javascript"><![CDATA[
scwin.onpageload = function() {
	scwin.fn_pageInit();
	grd_excelDown.setStyle("display" , "none");
};

scwin.onpageunload = function() {
		
	};

	scwin.category2ListSubmintdone = function(e) {
		dma_postParam.set("category1", parentId.getValue());
	};	
	
	//순번
	scwin.gridNoFormat = function(str, formattedValue, rowIdx, colIdx) {
    var rowSize = parseInt(dma_pageInfo.get("rowSize"), 10);
    var pageNo = parseInt(dma_pageInfo.get("pageNo"), 10);
    return rowSize * (pageNo - 1) + rowIdx + 1;
};

	// 게시글 검색
	scwin.btn_search_onclick = function(e) {
		scwin.fn_search(1);
	};
	
	// 초기화 버튼 클릭
	scwin.btn_init_onclick = function(e) {
		parentId.setValue("");
		dma_postParam.setEmptyValue();
	};
	
	// 카테고리 1d 변경될 때마다 2d 조회
	scwin.parentId_onchange = function() {
		$p.executeSubmission("sbm_categoryList2d");
	};
	
	scwin.slb_pagePerCount_onchange = function() {
		scwin.fn_search(1);
	};
	
	
	// 목록 조회
	scwin.fn_search = function(pageNo) {
		dma_postParam.set("size", rowSize.getValue());
		dma_postParam.set("page", pageNo);
		dma_pageInfo.set("rowSize", rowSize.getValue());
		dma_pageInfo.set("pageNo", pageNo);
		
		$p.executeSubmission("sbm_postList");
	};
	
	// 목록 초기화
	scwin.fn_pageInit = function() {
		dma_pageInfo.set("pageNo", 1);
		dma_pageInfo.set("rowSize", 5);
    	$p.executeSubmission("sbm_categoryList1d");
        $p.executeSubmission("sbm_postList");
        scwin.fn_showHide("");
	};

	
	scwin.sbm_postList_submitdone = function(e) {
		dma_pageInfo.set("totalPage", Math.ceil(dma_totalCount.get("totalCount") / dma_pageInfo.get("rowSize")));
		tbx_total.setValue(dma_totalCount.get("totalCount"));
		var pglFlag = true;
		if ("1" == dma_pageInfo.get("pageNo")) {
			pglFlag = false;
		}
		pgl_pageList.setCount(dma_pageInfo.get("totalPage"), pglFlag);
		pgl_pageList.setSelectedIndex(dma_pageInfo.get("pageNo"));
	};
	
	//페이징 처리
	scwin.pgl_pageList_onviewchange = function(info) {
		scwin.fn_search(info.newSelectedIndex);
	};
	
	//삭제 클릭
	scwin.btn_delete_onclick = function(e) {
		var checkColArr = dlt_postList.getMatchedIndex("check","1");
		if (checkColArr.length == 0) {
			alert("선택된 행이 없습니다.");
			return;
		}
		$p.executeSubmission("sbm_deletePostList");
	};
	
	scwin.sbm_deletePostList_submit = function(result) {
		var oList = dlt_postList.getMatchedJSON("check", "1");
		var postIdList = [];
		for (var i = 0; i < oList.length; i++) {
			postIdList[i] = oList[i].postId;
		}

		dma_deleteList.set("postIdList", postIdList);
	};
	
	scwin.sbm_deletePostList_submitdone = function(e) {
			if (e.responseStatusCode != "200" || e.responseJSON == "0") {
			console.log(e);
		} else {
			scwin.fn_search(1);
		}
	};

	scwin.sbm_updatePostList_submitdone = function(e) {
		if (e.responseStatusCode != "200" || e.responseJSON == "0") {
			console.log(e);
		} else {
			scwin.fn_search(1);
		}
	};

	
	scwin.btn_save_onclick = function(e) {
		var modifiedList = dlt_postList.getModifiedIndex();
		var categorySet = new Set();

	    for (var i = 0; i < modifiedList.length; i++) {
	        var category2 = dlt_postList.getCellData(modifiedList[i], "category2");
	        var displayOrder = dlt_postList.getCellData(modifiedList[i], "displayOrder");
	        
	        // displayOrder 값이 음수이거나 숫자가 아닐 경우
	        if (isNaN(displayOrder) || parseInt(displayOrder) < 0) {
	            alert("전시 순서는 0 이상의 숫자여야 합니다.");
	            scwin.fn_search(dma_pageInfo.get("pageNo"));
	            return;
	        }
	        categorySet.add(category2);
	    }
	    
	    if (categorySet.size < 1) {
		    alert("변경할 내용이 없습니다.");
		    return;
		   }
	    // category2 값이 서로 다르면 alert 창 띄우기
	    if (categorySet.size > 1) {
	        alert("전시 순서 변경 전 카테고리 2d를 먼저 검색해주세요."); 
	        scwin.fn_search(dma_pageInfo.get("pageNo"));
	        return;
	    }
	    
	    $p.executeSubmission("sbm_updatePostList");
		
	};
	
	//엑셀 다운로드
	scwin.btn_excelDown_onclick = function(e) {
		console.log("dd");
		var isExistParam = 0;
		
			var url = "http://localhost:8080";
		
			var sendUrl = url;
		    sendUrl += "/post/excel?";
		    if (dma_postParam.get('category1') != '') {
				sendUrl += "category1=" + dma_postParam.get('category1');
				isExistParam = 1;
			}
			if (dma_postParam.get('category2') != '') {
				sendUrl += "&category2=" + dma_postParam.get('category2');
				isExistParam = 1;
			}
			if (dma_postParam.get('periodType') != '') {
				sendUrl += "&periodType=" + dma_postParam.get('periodType');
				isExistParam = 1;
			}
			if (dma_postParam.get('startDate') != '') {
				sendUrl += "&startDate=" + dma_postParam.get('startDate');
				isExistParam = 1;
			}
			if (dma_postParam.get('endDate') != '') {
				sendUrl += "&endDate=" + dma_postParam.get('endDate');
				isExistParam = 1;
			}
			if (dma_postParam.get('isDisplayed') != '') {
				sendUrl += "&isDisplayed=" + dma_postParam.get('isDisplayed');
				isExistParam = 1;
			}
			if (dma_postParam.get('searchType') != '') {
				sendUrl += "&searchType=" + dma_postParam.get('searchType');
				isExistParam = 1;
			}
			if (dma_postParam.get('keyword') != '') {
				sendUrl += "&keyword=" + dma_postParam.get('keyword');
				isExistParam = 1;
			}
			
			if (isExistParam == 0) {
				sendUrl = "http://localhost:8080/post/excel";
			}
			console.log(sendUrl);
		try{
			$p.download(sendUrl);
			console.log("sdfa");
		} catch(e) {
			concole.log(e);
		}
	
		
	
	};
	
	// 달력 숨기기
	scwin.periodType_onviewchange = function(info) {
		scwin.fn_showHide(info.newValue);
	};
	
	//검색영역 show,hide
	scwin.fn_showHide = function(cur) {
		if (cur == null || cur == "") {
			startDate.setStyle("display" , "none");
			endDate.setStyle("display" , "none");
			wave.setStyle("display" , "none");
		} else {
			startDate.setStyle("display" , "");
			endDate.setStyle("display" , "");
			wave.setStyle("display" , "");
		}
	};
	
	// 등록 버튼 클릭 시 페이지 이동
	scwin.btn_reg_onclick = function(e) {
		var tmpUrl = "&w2Config=/.settings/config.xml";
		$p.url("/post_registration.xml");
	};
]]></script>
    </head>
