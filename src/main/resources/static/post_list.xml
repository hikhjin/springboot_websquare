        		mediatype="application/json" method="post" mode="asynchronous" processMsg=""
        		ref='data:json,{"id":"dma_parentCategory","key":"parentCategory"}' replace=""
        		target='data:json,{"id":"dlt_categoryList2d","key":"categoryList2d"}' style="">
        	</xf:submission>
        	<xf:submission id="sbm_deletePostList" ref='data:json,{"id":"dma_deleteList","key":"deleteList"}' target=""
        		action="http://127.0.0.1:8080/post" method="delete" mediatype="application/json" encoding="UTF-8" instance="" replace=""
        		errorHandler="" customHandler="" mode="asynchronous" processMsg="" ev:submit="scwin.sbm_deletePostList_submit"
        		ev:submitdone="scwin.sbm_deletePostList_submitdone" ev:submiterror="" abortTrigger="" singleMode="true">
        	</xf:submission>
        	<xf:submission id="sbm_updatePostList" ref='data:json,{"id":"dlt_postList","key":"postList"}' target=""
